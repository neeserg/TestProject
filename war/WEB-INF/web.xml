<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

    
	<servlet>
		<servlet-name>TestProject</servlet-name>
		<servlet-class>com.test.project.TestProjectServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestProject</servlet-name>
		<url-pattern>/testproject</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RegisterUserServlet</servlet-name>
		<servlet-class>com.test.project.RegisterUserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterUserServlet</servlet-name>
		<url-pattern>/registerUser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RegisterHotelServlet</servlet-name>
		<servlet-class>com.test.project.RegisterHotelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterHotelServlet</servlet-name>
		<url-pattern>/registerHotel</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RateHotel</servlet-name>
		<servlet-class>com.test.project.RateHotelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RateHotel</servlet-name>
		<url-pattern>/rateHotel</url-pattern>
	</servlet-mapping>
	
	
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
